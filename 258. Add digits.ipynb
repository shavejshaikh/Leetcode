    "class Solution:\n",
    "    def addDigits(self, num):\n",
    "        print(f\"Add Digits {num}\")\n",
    "        \n",
    "        while num!= 0:\n",
    "            for i in str(num):\n",
    "                print(i)\n",
    "            num = num // 2\n",
    "            print(f\"Digit is : {num}\")\n",
    "sol = Solution()\n",
    "num = 38\n",
    "sol.addDigits(num)"
   ]
