    "class Solution:\n",
    "    def xorOperation(self, n: int, start: int) -> int:\n",
    "        xor = 0\n",
    "        for i in range(0, n):\n",
    "            temp = start+2*i\n",
    "            xor = xor ^ temp\n",
    "        return xor\n",
    "\n",
    "sol = Solution()\n",
    "n = 5\n",
    "start = 0\n",
    "sol.xorOperation(n, start)"
   ]
