   "source": [
    "class Solution:\n",
    "    def singleNumber(self, nums):\n",
    "        xor = 0\n",
    "        for index in nums:\n",
    "            xor = xor ^ index\n",
    "            print(f\"Index is : {index}--- {xor}\")\n",
    "            \n",
    "        print(xor)\n",
    "\n",
    "nums = [2, 2, 1]\n",
    "sol = Solution()\n",
    "sol.singleNumber(nums)"
   ]
